<manifest>
    <main-class>com.xdxdxdcat.bots.herbCleaner.HerbCleaner</main-class>
    <name>XDXDXDCat's Herb Cleaner</name>
    <tag-line>Cleans all herbs quickly!</tag-line><!--Max of 50 chars-->
    <description>Cleans any herb you set it to clean.</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>HERBLORE</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>HerbCleaner</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Herb</tag>
        <tag>Cleaning</tag>
    </tags>
</manifest>